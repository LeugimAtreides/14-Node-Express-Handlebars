<div class="container valign-wrapper">
    <div class="row">
        <div class="col s4 center-align">
            <div class="card-panel orange lighten-4">
                <h1>Try A Burger On Us!</h1>
                <ul>
                    {{#each burger}}
                        {{#unless devoured}}
                            {{> burger/burger-block devoured=true}}
                        {{/unless}}
                    {{/each}}
                </ul>
            </div>
        </div>
        <div class="col s4 center-align">
            <div class="card-panel" id="colors">
                <h1>{{burgerGIF}}</h1>
            </div>
        </div>
        <div class="col s4 center-align">
            <div class="card-panel orange lighten-4">
                <h1>The Burgers You Ate!</h1>
                <ul>
                    {{#each burger}}
                        
                </ul>
            </div>
        </div>
    </div>
</div>